{
  "name": "soundtools-proxy",
  "version": "1.0.0",
  "description": "Contentful API proxy server for SoundTools website",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "dev:watch": "nodemon --watch src --ext ts,js --exec ts-node src/index.ts",
    "heroku-postbuild": "npm run build"
  },
  "keywords": ["proxy", "contentful", "api", "nodejs", "typescript"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "node-cache": "^5.1.2",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.4",
    "@types/node-cache": "^4.2.5",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
